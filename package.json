{
  "name": "precharts",
  "amdName": "precharts",
  "version": "1.4.0",
  "description": "Wraps Recharts up for Preact, without preact-compat",
  "main": "dist/precharts.js",
  "src:main": "src/index.js",
  "minified:main": "dist/precharts.min.js",
  "scripts": {
    "clean": "rimraf dist/",
    "build": "NODE_ENV=production npm-run-all clean transpile minify size",
    "transpile": "rollup -c rollup.config.js -m ${npm_package_main}.map -f umd -n $npm_package_amdName $npm_package_src_main -o $npm_package_main",
    "minify": "uglifyjs $npm_package_main --define NODE_ENV=production --pure-funcs classCallCheck Object.defineProperty Object.freeze invariant warning -c unsafe,collapse_vars,evaluate,screw_ie8,loops,keep_fargs=false,pure_getters,unused,dead_code -m -o $npm_package_minified_main -p relative --in-source-map ${npm_package_main}.map --source-map ${npm_package_minified_main}.map",
    "size": "size=$(gzip-size $npm_package_minified_main) && echo \"gzip size: $size / $(pretty-bytes $size)\"",
    "test": "NODE_ENV=development npm-run-all lint transpile test:karma",
    "lint": "eslint {src,test}",
    "test:karma": "karma start --single-run",
    "test:watch": "karma start",
    "prepublish": "npm-run-all build test",
    "release": "npm run build && git commit -am $npm_package_version && git tag $npm_package_version && git push && git push --tags && npm publish"
  },
  "keywords": [
    "preact",
    "recharts",
    "precharts"
  ],
  "author": "Jason Miller <jason@developit.ca>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/developit/precharts.git"
  },
  "bugs": {
    "url": "https://github.com/developit/precharts/issues"
  },
  "homepage": "https://github.com/developit/precharts",
  "peerDependencies": {
    "preact": ">=5"
  },
  "devDependencies": {
    "babel-cli": "^6.24.0",
    "babel-core": "^6.24.0",
    "babel-eslint": "^7.1.1",
    "babel-loader": "^6.4.1",
    "babel-plugin-external-helpers": "^6.22.0",
    "babel-plugin-transform-class-properties": "^6.23.0",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-plugin-transform-es2015-classes": "^6.23.0",
    "babel-plugin-transform-node-env-inline": "^0.0.2",
    "babel-plugin-transform-object-assign": "^6.22.0",
    "babel-plugin-transform-react-jsx": "^6.23.0",
    "babel-plugin-transform-react-remove-prop-types": "^0.3.2",
    "babel-preset-es2015": "^6.24.0",
    "babel-preset-react": "^6.23.0",
    "babel-preset-stage-0": "^6.22.0",
    "chai": "^3.5.0",
    "diff": "^3.2.0",
    "eslint": "^3.18.0",
    "eslint-plugin-react": "^6.10.0",
    "gzip-size-cli": "^2.0.0",
    "karma": "^1.5.0",
    "karma-chai-sinon": "^0.1.5",
    "karma-mocha": "^1.3.0",
    "karma-mocha-reporter": "^2.2.2",
    "karma-phantomjs-launcher": "^1.0.2",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-webpack": "^2.0.2",
    "mkdirp": "^0.5.1",
    "mocha": "^3.2.0",
    "npm-run-all": "^4.0.2",
    "phantomjs-prebuilt": "^2.1.14",
    "pretty-bytes-cli": "^2.0.0",
    "recharts": "^0.21.2",
    "rimraf": "^2.6.1",
    "rollup": "^0.40.1",
    "rollup-plugin-alias": "^1.2.0",
    "rollup-plugin-babel": "^2.7.1",
    "rollup-plugin-commonjs": "^7.0.0",
    "rollup-plugin-memory": "^2.0.0",
    "rollup-plugin-replace": "^1.1.1",
    "sinon": "^1.17.7",
    "sinon-chai": "^2.8.0",
    "uglify-js": "^2.8.7",
    "webpack": "^1.14.0"
  },
  "dependencies": {
    "classnames": "^2.2.5",
    "core-js": "^2.4.1",
    "d3-scale": "^1.0.5",
    "d3-shape": "^1.0.6",
    "element-resize-detector": "^1.1.10",
    "eventemitter3": "^2.0.2",
    "lodash": "^4.17.4",
    "preact": "^7.2.0",
    "preact-compat": "^3.13.1",
    "preact-transition-group": "^1.1.0",
    "raf": "^3.3.0",
    "reduce-css-calc": "^1.3.0"
  }
}
